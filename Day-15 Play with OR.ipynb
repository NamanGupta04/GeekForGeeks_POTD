    "class Solution:\n",
    "    def game_with_number (self, arr,  n) : \n",
    "        #Complete the function\n",
    "        for i in range(n - 1):\n",
    "            arr[i] = arr[i] | arr[i + 1]\n",
    "        \n",
    "        return arr"
